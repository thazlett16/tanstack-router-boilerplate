{
    "compilerOptions": {
        "strict": true,
        "esModuleInterop": true,
        "jsx": "react-jsx",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "@e2e/*": ["./e2e/*"],
            "@mock/*": ["./mock/*"],
            "@test/*": ["./test/*"]
        }
    }
}
